{
  "name": "@mango/event-emitter",
  "version": "1.0.0",
  "description": "事件订阅发布库",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "esm",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-s clean lib",
    "clean": "rimraf lib esm",
    "lib": "run-p lib:*",
    "lib:cjs": "tsc -p tsconfig.json --target ES5 --module commonjs --outDir lib",
    "lib:esm": "tsc -p tsconfig.json --target ES5 --module ESNext --outDir esm",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mangobuaa/event-emitter.git"
  },
  "keywords": [
    "event",
    "emitter",
    "事件",
    "订阅",
    "发布"
  ],
  "author": "MangoLee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mangobuaa/event-emitter/issues"
  },
  "homepage": "https://github.com/mangobuaa/event-emitter#readme",
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.22",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
